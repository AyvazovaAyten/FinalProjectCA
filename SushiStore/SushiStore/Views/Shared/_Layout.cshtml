<!DOCTYPE html>
<html lang="en">

<head runat="server">
    <meta charset="UTF-8">
    <title>@RenderSection("Title", false) @ViewData["DefaultTitle"]</title>



    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport"
          content="width=device-width, height=device-height, initial-scale=1.0, user-scalable=1, minimum-scale=1.0, maximum-scale=1.0">
    <meta name="mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-touch-fullscreen" content="yes">
    <link rel="shortcut icon" href="#">

    @await Component.InvokeAsync("Tags")
    @RenderSection("Tags", false)

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="~/vendors/slick/slick.css">
    <link rel="stylesheet" href="~/vendors/slick/slick-theme.css">



    <link href="~/assets/css/style.css" rel="stylesheet" type="text/css" />


    <script src="https://kit.fontawesome.com/bce32b97fb.js" crossorigin="anonymous"></script>

    <script src="~/Scripts/jquery/dist/jquery.js" type="text/javascript"></script>
    <asp:ContentPlaceHolder id="Header" runat="server"></asp:ContentPlaceHolder>

    <script src="~/vendors/slick/slick.min.js" type="text/javascript"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"></script>
    @await Component.InvokeAsync("CSS")
</head>

<body>

    <div class="main_wrapper">

        @await Component.InvokeAsync("Header")

        @RenderBody()

        @await Component.InvokeAsync("Footer")

    </div>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" type="text/javascript"></script>
    <script src="~/js/common.js" type="text/javascript"></script>
    @RenderSection("Script", false)

    @await Component.InvokeAsync("JS")
</body>

</html>
